{
  "name": "vue-livr",
  "version": "0.1.2",
  "description": "Vue LIVR plugin",
  "author": "Jonatas Gusm√£o <js.gusmao@hotmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist",
    "dist/esm",
    "dist/cjs"
  ],
  "keywords": [
    "Vue",
    "VueJS",
    "LIVR",
    "validation",
    "validator",
    "inputs",
    "form"
  ],
  "scripts": {
    "prepare": "rollup --config",
    "lint": "eslint src -c .eslintrc.js --ext .js",
    "lintfix": "eslint src -c .eslintrc.js --fix --ext .js",
    "test": "jest --config test/unit/jest.conf.js"
  },
  "dependencies": {
    "livr": "^2.2.5",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.4.5",
    "babel-jest": "^24.8.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-graphql": "^3.0.3",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-module-resolver": "^0.9.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.2.2",
    "husky": "^1.3.1",
    "jest": "^24.8.0",
    "pre-commit": "1.2.2",
    "prettier": "^1.17.1",
    "rollup": "^1.15.1",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-terser": "^5.0.0",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-merge": "^4.2.1"
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">= 6.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "eslint"
    ],
    "*.{js,json,md}": [
      "prettier --list-different"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grupozap/vue-livr.git"
  }
}
